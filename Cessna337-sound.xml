<?xml version="1.0" encoding="UTF-8"?>

<PropertyList>
<fx>

<!-- ENGINE -->

	<!-- FRONT ENGINE -->

	<crank>
		<name>engstart1</name>
		<path>Sounds/engstart.wav</path>
		<property>engines/engine[0]/cranking</property>
		<position>
			<x>-1.8</x>
			<y> 0  </y>
			<z> 0  </z>
		</position>
		<volume>
			<offset>0.5</offset>
		</volume>
	</crank>
	<crank>
		<name>crank1</name>
		<mode>looped</mode>
		<path>Sounds/cranking.wav</path>
		<condition>
			<property>engines/engine[0]/cranking</property>
			<not>
				<property>engines/engine[0]/running</property>
			</not>
		</condition>
		<position>
			<x>-1.8</x>
			<y> 0  </y>
			<z> 0  </z>
		</position>
		<volume>
			<offset>0.5</offset>
		</volume>
	</crank>
	<crank>
		<name>cough1</name>
		<path>Sounds/coughing.wav</path>
		<property>engines/engine[0]/running</property>
		<position>
			<x>-1.8</x>
			<y> 0  </y>
			<z> 0  </z>
		</position>
		<volume>
			<offset>0.3</offset>
		</volume>
	</crank>

	<engine>
		<name>engine-external-1</name>
		<mode>looped</mode>
		<path>Aircraft/Cessna337/Sounds/Engine_Continental-O200A.wav</path>
		<position>
			<x>-1.8</x>
			<y> 0  </y>
			<z> 0  </z>
		</position>
		<condition>
			<property>engines/engine[0]/running</property>
			<not>
				<property>/sim/current-view/internal</property>
			</not>
		</condition>
		<volume>
			<property>engines/engine[0]/mp-osi</property>
			<factor>0.018</factor>
			<min>0.35</min>
			<max>2</max>
			<offset>0.35</offset>
		</volume>
		<pitch>
			<property>engines/engine[0]/rpm</property>
			<factor>0.0005</factor>
			<min>0.3</min>
			<max>2.0</max>
			<offset>0.3</offset>
		</pitch>
	</engine>

	<engine>
		<name>engine-internal-1</name>
		<mode>looped</mode>
		<path>Aircraft/Cessna337/Sounds/Engine_Continental-O200A-LPF.wav</path>
		<condition>
			<and>
				<property>engines/engine[0]/running</property>
				<property>/sim/current-view/internal</property>
			</and>	
		</condition>
		<position>
			<x>-1.8</x>
			<y> 0  </y>
			<z> 0  </z>
		</position>
		<volume>
			<property>engines/engine[0]/mp-osi</property>
			<factor>0.012</factor>
			<min>0.4</min>
			<max>1.5</max>
			<offset>0.4</offset>
		</volume>
		<pitch>
			<property>engines/engine[0]/rpm</property>
			<factor>0.0005</factor>
			<min>0.3</min>
			<max>2.0</max>
			<offset>0.3</offset>
		</pitch>
	</engine>

	<!-- REAR ENGINE -->

	<crank>
		<name>engstart2</name>
		<path>Sounds/engstart.wav</path>
		<property>engines/engine[1]/cranking</property>
		<position>
			<x>2.3</x>
			<y>0  </y>
			<z>0.3</z>
		</position>
		<volume>
			<offset>0.5</offset>
		</volume>
	</crank>
	<crank>
		<name>crank2</name>
		<mode>looped</mode>
		<path>Sounds/cranking.wav</path>
		<condition>
			<property>engines/engine[1]/cranking</property>
			<not>
				<property>engines/engine[1]/running</property>
			</not>
		</condition>
		<position>
			<x>2.3</x>
			<y>0  </y>
