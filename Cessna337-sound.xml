		<position>
			<x>2.3</x>
			<y>0  </y>
			<z>0.3</z>
		</position>
		<volume>
			<offset>0.3</offset>
		</volume>
	</crank>

	<engine>
		<name>engine-external-2</name>
		<mode>looped</mode>
		<path>Aircraft/Cessna337/Sounds/Engine_Continental-O200A.wav</path>
		<position>
			<x>2.3</x>
			<y>0  </y>
			<z>0.3</z>
		</position>
