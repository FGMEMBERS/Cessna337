			<offset>0.3</offset>
		</pitch>
	</engine>

	<!-- FLAPS & CONTROLS -->

	<flap>
		<name>flaps</name>
		<mode>in-transit</mode>
		<path>Aircraft/Cessna337/Sounds/Cessna150Flaps.wav</path>
		<property>surface-positions/flap-pos-norm</property>
		<volume>
			<factor>0.8</factor>
		</volume>
		<reference-dist>10.0</reference-dist>
		<max-dist>20.0</max-dist>
	</flap>

	<rudder>
		<name>Rudder pedals noise L</name>
		<path>Aircraft/Cessna337/Sounds/RudderPedalNoise.wav</path>
		<condition>
			<less-than>
				<property>controls/flight/rudder</property>
				<value>-0.25</value>
			</less-than>
		</condition>
		<volume>
			<factor>0.75</factor>
		</volume>
	</rudder>

	<rudder>
		<name>Rudder pedals noise R</name>
		<path>Aircraft/Cessna337/Sounds/RudderPedalNoise.wav</path>
		<condition>
			<greater-than>
				<property>controls/flight/rudder</property>
				<value>0.25</value>
			</greater-than>
		</condition>
		<volume>
			<factor>0.75</factor>
		</volume>
	</rudder>

	<!-- DOOR -->

	<door>
		<name>Door Open</name>
		<path>Aircraft/Cessna337/Sounds/DoorOpen.wav</path>
		<condition>
			<greater-than>
				<property>controls/door</property>
				<value>0.1</value>
			</greater-than>
		</condition>
		<volume>
			<factor>0.8</factor>
		</volume>
	</door>
	<door>
		<name>Door Close</name>
		<path>Aircraft/Cessna337/Sounds/DoorClosed.wav</path>
		<condition>
			<equals>
				<property>controls/door</property>
				<value>0</value>
			</equals>
		</condition>
		<volume>
			<factor>1</factor>
		</volume>
	</door>

	<!-- RUMBLE & SQUEAL -->

	<rumble>
		<name>Rumble</name>
		<mode>looped</mode>
		<path>Sounds/rumble.wav</path>
		<condition>
			<or>
				<property>gear/gear[0]/wow</property>
				<property>gear/gear[1]/wow</property>
				<property>gear/gear[2]/wow</property>
			</or>
		</condition>
		<volume>
			<property>velocities/groundspeed-kt</property>
			<type>log</type>
			<factor>0.35</factor>
		</volume>
	</rumble>

	<squeal>
		<name>Squeal</name>
		<path>Aircraft/Cessna337/Sounds/Squeal.wav</path>
		<condition>
			<or>
				<property>gear/gear[0]/wow</property>
				<property>gear/gear[1]/wow</property>
				<property>gear/gear[2]/wow</property>
			</or>
		</condition>
		<volume>
			<factor>0.8</factor>
		</volume>
		<pitch>
			<property>velocities/groundspeed-kt</property>
			<factor>0.0025</factor>
			<offset>1.2</offset>
		</pitch>
		<position>
			<x> 0.8 </x>
			<y> 0   </y>
			<z>-1.28</z>
		</position>
		<reference-dist>20.0</reference-dist>
		<max-dist>50.0</max-dist>
	</squeal>

	<!-- CRASH & ALARMS -->

	<crash>
		<name>Intense ground contact</name>
		<mode>once</mode>
		<path>Sounds/crash.wav</path>
		<property>sim/crashed</property>
		<volume>
			<factor>1</factor>
		</volume>
		<pitch>
			<factor>0.1</factor>
		</pitch>
		<reference-dist>100</reference-dist>
		<max-dist>500</max-dist>
	</crash>

	<stall>
		<name>stall</name>
		<mode>looped</mode>
		<path>Aircraft/Cessna337/Sounds/Stall.wav</path>
		<condition>
			<and>
				<or>
					<property>controls/engines/engine/master-bat</property>
					<property>controls/engines/engine[0]/master-alt</property>
					<property>controls/engines/engine[1]/master-alt</property>
				</or>
				<less-than>
					<property>velocities/airspeed-kt</property>
					<value>65</value>
				</less-than>
				<not>
					<property>gear/gear[0]/wow</property>
					<property>gear/gear[1]/wow</property>
					<property>gear/gear[2]/wow</property>
				</not>
			</and>
		</condition>
		<volume>
			<factor>0.7</factor>
		</volume>
		<reference-dist>4.0</reference-dist>
		<max-dist>10.0</max-dist>
	</stall>

	<gear-warn>
		<name>gear warning</name>
		<mode>looped</mode>
		<path>Sounds/gear-hrn.wav</path>
		<condition>
			<or>
				<and>
					<property>gear/gear[1]/wow</property>
					<not>
						<property>controls/gear/gear-down</property>
					</not>
				</and>
				<and>
					<or>
						<less-than>
							<property>gear/gear[0]/position-norm</property>
							<value>0.95</value>
						</less-than>
						<less-than>
							<property>gear/gear[1]/position-norm</property>
							<value>0.95</value>
						</less-than>
						<less-than>
							<property>gear/gear[2]/position-norm</property>
							<value>0.95</value>
						</less-than>
					</or>
					<or>
						<less-than>
							<property>controls/engines/engine[0]/throttle</property>
							<value>0.2</value>
						</less-than>
						<less-than>
							<property>controls/engines/engine[1]/throttle</property>
							<value>0.2</value>
						</less-than>
					</or>
					<or>
						<less-than>
							<property>/position/altitude-agl-ft</property>
							<value>500</value>
						</less-than>
					</or>
				</and>
			</or>
		</condition>
		<position>
			<x>-2.0</x>
			<y>0.40</y>
			<z>1.40</z>
		</position>
		<volume>
			<factor>0.6</factor>
		</volume>
		<reference-dist>2.0</reference-dist>
		<max-dist>5.0</max-dist>
	</gear-warn>

        <gear>
            <name>gear</name>
            <mode>in-transit</mode>
            <path>Sounds/gear.wav</path>
            <property>gear/gear[0]/position-norm</property>
            <pitch>
                <internal>dt_play</internal>
                <offset>1.0</offset>
                <factor>-0.001</factor>
            </pitch>
            <reference-dist>10.0</reference-dist>
            <max-dist>20.0</max-dist>
        </gear>
        <gear>
            <name>gear-lock</name>
            <path>Sounds/gear-lck.wav</path>
            <condition>
                <equals>
                    <property>gear/gear[0]/position-norm</property>
                    <value>0</value>
                </equals>
            </condition>
            <reference-dist>10.0</reference-dist>
            <max-dist>20.0</max-dist>
        </gear>

	<wind>
		<name>wind</name>
		<mode>looped</mode>
		<path>Sounds/wind.wav</path>
		<property>velocities/airspeed-kt</property>
		<volume>
			<property>position/altitude-ft</property>
			<factor>-0.000015</factor>
			<offset>0.4</offset>
			<min>0.1</min>
			<max>0.4</max>
		</volume>
		<volume>
			<property>velocities/airspeed-kt</property>
			<factor>0.0007</factor>
			<min>0.03</min>
			<max>0.12</max>
		</volume>
		<pitch>
			<property>velocities/airspeed-kt</property>
			<factor>0.01</factor>
			<offset>1.25</offset>
		</pitch>
	</wind>

	<!-- SWITCHES -->

		<!-- BATTERY -->

	<click> 
		<name>Battery ON (switch)</name>
		<path>Aircraft/Cessna337/Sounds/ButtonPower.wav</path>
		<condition> 
			<property>controls/engines/engine/master-bat</property>
		</condition>
		<volume> 
			<factor>0.7</factor>
		</volume>
	</click>
	<click> 
		<name>Battery ON (noise)</name>
		<path>Aircraft/Cessna337/Sounds/BatteryStart.wav</path>
		<condition> 
			<property>controls/engines/engine/master-bat</property>
		</condition>
		<volume> 
			<factor>0.2</factor>
		</volume>
	</click>
	<click> 
		<name>Battery OFF</name>
		<path>Aircraft/Cessna337/Sounds/ButtonPower.wav</path>
		<condition> 
			<not> 
				<property>controls/engines/engine/master-bat</property>
			</not>
		</condition>
		<volume> 
			<factor>0.7</factor>
		</volume>
	</click>

